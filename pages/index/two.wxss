.ttwrap{
  height:150rpx;
  position: absolute;
  width:100%;
  top:0;
  opacity: 0;
  animation: 0.8s 0s linear forwards
}
.two .ttwrap{
  animation-name: ttwrap
}
@keyframes ttwrap {
  from,
  60%,
  75%,
  90%,
  to {
    animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
  }

  0% {
    opacity: 0;
    transform: translate3d(0, -3000px, 0);
  }

  60% {
    opacity: 1;
    transform: translate3d(0, 25px, 0);
  }

  75% {
    opacity: 1;
    transform: translate3d(0, -10px, 0);
  }

  90% {
    opacity: 1;
    transform: translate3d(0, 5px, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
.ttop{
  transform: rotate(180deg) scale(1.3,1);
  position: absolute;
  bottom:0;
  width:100%;
}
.tcot{
  position:absolute;
  width: 90%;
  height:900rpx;
  top:150rpx;
  left:50%;
  transform: translateX(-50%);
  transform: -webkit-translateX(-50%);
  box-sizing: border-box;
  padding:30rpx 100rpx 200rpx;
}
.tbg{
  width:100%;
  height:100%;
  position:absolute;
  top:0;
  left:0;
  z-index: -1;
  transform-origin: left 80%;
  transform: rotate(-120deg);
  opacity: 0;
  animation: 0.8s 0.4s linear forwards;
}
.two .tbg{
  animation-name: tbg
}
@keyframes tbg{
  from{}
  50%{
    opacity:1;
  }
  to{
    opacity: 1;
    transform: rotate(0deg);
  }
}
.tbird{
  width:150rpx;
  position: absolute;
  left:-20rpx;
  top:-20rpx;
  opacity: 0;
  animation: 1s 2.8s linear forwards;
}
.two .tbird{
  animation-name: tbird;
}
@keyframes tbird {
  from,
  60%,
  75%,
  90%,
  to {
    animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
  }

  0% {
    opacity: 0;
    transform: translate3d(-3000px, 0, 0);
  }

  60% {
    opacity: 1;
    transform: translate3d(25px, 0, 0);
  }

  75% {
    opacity: 1;
    transform: translate3d(-10px, 0, 0);
  }

  90% {
    opacity: 1;
    transform: translate3d(5px, 0, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
.thua{
  width:230rpx;
  position: absolute;
  right:-80rpx;
  top:-130rpx;
  opacity: 0;
  animation: 1s 2.8s linear forwards;
}
.two .thua{
  animation-name: thua
}
@keyframes thua {
  from,
  60%,
  75%,
  90%,
  to {
    animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
  }

  from {
    opacity: 0;
    transform: translate3d(3000px, 0, 0);
  }

  60% {
    opacity: 1;
    transform: translate3d(-25px, 0, 0);
  }

  75% {
    opacity: 1;
    transform: translate3d(10px, 0, 0);
  }

  90% {
    opacity: 1;
    transform: translate3d(-5px, 0, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
.tlqq,.trqq{
  width:120rpx;
  position: absolute;
}
.tlqq{
  left:-20rpx;
  top:180rpx;
  opacity: 0;
  animation: 0.8s 2s linear forwards;
}
.two .tlqq{
  animation-name: tqq;
}
.trqq{
  bottom:150rpx;
  right:-30rpx;
  opacity: 0;
  animation: 0.8s 2s linear forwards;
}
.two .trqq{
  animation-name: tqq;
}
@keyframes tqq {
  from {
    opacity: 0;
    transform: translate3d(0, 100%, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
.thh{
  width:120rpx;
  position: absolute;
  left:0;
  bottom:100rpx;
  opacity: 0;
  animation: 0.8s 1s linear forwards;
}
.two .thh{
  animation-name: thh
}

@keyframes thh {
  from {
    transform-origin: center;
    transform:translate3d(-100rpx, -100rpx, 0) rotate3d(0, 0, 1, -200deg);
    opacity: 0;
  }

  to {
    transform-origin: center;
    transform: translate3d(0, 0, 0);
    opacity: 1;
  }
}

.tfoot{
  position: absolute;
  bottom:0;
  left:0;
  width:100%;
  height:120rpx;
}
.tmuban{
  width: 100%;
  display: block;
  opacity: 0;
  animation: 0.8s 2.1s linear forwards
}
.two .tmuban{
  animation-name: tmuban
}
@keyframes tmuban {
  from,
  60%,
  75%,
  90%,
  to {
    animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
  }

  from {
    opacity: 0;
    transform: translate3d(0, 3000px, 0);
  }

  60% {
    opacity: 1;
    transform: translate3d(0, -20px, 0);
  }

  75% {
    opacity: 1;
    transform: translate3d(0, 10px, 0);
  }

  90% {
    opacity: 1;
    transform: translate3d(0, -5px, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
.tmxrz{
  width:70%;
  position: absolute;
  bottom:80rpx;
  left: 50%;
  transform: translateX(-50%);
  transform: -webkit-translateX(-50%);
  opacity: 0;
  animation: 1s 2.8s ease-out forwards;
}
.two .tmxrz{
  animation-name: tmxrz;
}
@keyframes tmxrz {
  from,
  60%,
  75%,
  90%,
  to {
    animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
  }

  from {
    opacity: 0;
    transform: translate3d(-50%, 3000px, 0);
  }

  60% {
    opacity: 1;
    transform: translate3d(-50%, -20px, 0);
  }

  75% {
    opacity: 1;
    transform: translate3d(-50%, 10px, 0);
  }

  90% {
    opacity: 1;
    transform: translate3d(-50%, -5px, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(-50%, 0, 0);
  }
}
.tdetail{
  text-align: center;
  white-space: pre-line;
  font-size: 32rpx;
  line-height: 58rpx;
  transform: scale(0.5);
  opacity: 0;
  animation: 0.6s 1.1s linear forwards;
}
.two .tdetail{
  animation-name: tdetail
}
@keyframes tdetail{
  from{
    opacity: 0.5;
  }
  10%{
    opacity: 1;
  }
  to{
    transform: scale(1);
    opacity: 1;
  }
}