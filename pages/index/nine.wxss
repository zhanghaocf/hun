}
.nine .rqq9{
  animation: ninefadeinl 0.8s 1.8s linear forwards;
}
.nine .gl9{
  animation: ninerotatel 0.8s 2.2s linear forwards;
}
@keyframes ninerollin{
  from {
    opacity: 0;
    transform: translate3d(-100%, 0, 0) rotate3d(0, 0, 1, -120deg);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
@keyframes ninebounceinl{
  from,
  60%,
  75%,
  90%,
  to {
    animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
  }

  0% {
    opacity: 0;
    transform: translate3d(-3000px, 0, 0);
  }

  60% {
    opacity: 1;
    transform: translate3d(25px, 0, 0);
  }

  75% {
    opacity: 1;
    transform: translate3d(-10px, 0, 0);
  }

  90% {
    opacity: 1;
    transform: translate3d(5px, 0, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
@keyframes ninebounceinr{
  from,
  60%,
  75%,
  90%,
  to {
    animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
  }

  from {
    opacity: 0;
    transform: translate3d(3000px, 0, 0);
  }

  60% {
    opacity: 1;
    transform: translate3d(-25px, 0, 0);
  }

  75% {
    opacity: 1;
    transform: translate3d(10px, 0, 0);
  }

  90% {
    opacity: 1;
    transform: translate3d(-5px, 0, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
@keyframes ninefadeinu{
  from {
    opacity: 0;
    transform: translate3d(0, 100%, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
@keyframes ninerotate360l{
  from {
    transform-origin: center;
    transform:translateX(-100%) rotate(-360deg);
    opacity: 0;
  }

  to {
    transform-origin: center;
    opacity: 1;
  }
}
@keyframes ninerotatel{
  from {
    transform-origin: center;
    transform:translateX(-100%) rotate(-180deg);
    opacity: 0;
  }

  to {
    transform-origin: center;
    opacity: 1;
  }
}
@keyframes ninefadeinl{
  from {
    opacity: 0;
    transform: translate3d(-100%, 0, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
@keyframes ninefadeinr{
  from {
    opacity: 0;
    transform: translate3d(100%, 0, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
