.xrz7{
  width: 100rpx;
  position: absolute;
  bottom: 140rpx;
  left: 40rpx;
  opacity: 0;
}
.lh7{
  width: 140rpx;
  position: absolute;
  bottom: 140rpx;
  right: 20rpx;
  opacity: 0;
}
.wrap7{
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  height:100%;
  justify-content: space-between;
}
.middle7{
  flex-grow: 1;
  position: relative;
}
.foot7{
  flex-shrink: 0;
  position: relative;
  margin-bottom: -40rpx;
}
.top7{
  flex-shrink: 0;
  position: relative;
  text-align: center;
  z-index: 2;
  padding-top: 40rpx;
}
.topfont7{
  width:250rpx;
  opacity: 0;
}
.tl7{
  position: absolute;
  width:220rpx;
  left:0;
  top:0;
  opacity: 0;
}
.tr7{
  position: absolute;
  width:220rpx;
  right:0;
  top:0;
  z-index: 1;
  opacity: 0;
}
.tbird7{
  width:120rpx;
  position: absolute;
  top:130rpx;
  left:90rpx;
  opacity: 0;
}
.tflower7{
  width:120rpx;
  position: absolute;
  right:20rpx;
  top:120rpx;
  opacity: 0;
}
.middel7bg{
  width:100%;
  height:100%;
  position: absolute;
  top:0;
  left:0;
  z-index: -1;
  opacity: 0;
}
.sevenimg{
  width:500rpx;
  height:700rpx;
  position: absolute;
  box-sizing: border-box;
  border:5rpx solid white;
  top:50%;
  left:50%;
  margin-left: -250rpx;
  margin-top: -350rpx;
  opacity: 0;
}
.seven .middel7bg{
  animation: sevenfadedownscale 1s linear forwards;
}
.seven .tr7{
  animation: sevenbounceinr 0.8s 1s linear forwards;
}
.seven .topfont7{
  animation: sevenflipiny 2.5s 1.2s linear forwards;
}
.seven .tl7{
  animation: sevenbounceinl 1.5s 2s linear forwards;
}
.seven .tflower7{
  animation: sevendiao 1s 1.6s linear forwards,sevenzhuan 3s 2.6s linear infinite;
}
.seven .tbird7{
  animation:sevenfadeinl 0.8s 3.5s linear forwards;
}
.seven .sevenimg{
  animation: sevenrotateinr 0.8s 3.8s linear forwards;
}
.seven .tmuban{
  animation: sevenbounceinu 1.5s 0.8s linear forwards;
}
.seven .xrz7{
  animation: sevenbounceinl 1s 4.6s linear forwards
}
.seven .lh7{
  animation: sevenbounceinr 1s 4.6s linear forwards
}
@keyframes sevenbounceinu{
from,
  60%,
  75%,
  90%,
  to {
    animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
  }

  from {
    opacity: 0;
    transform: translate3d(0, 3000px, 0);
  }

  60% {
    opacity: 1;
    transform: translate3d(0, -20px, 0);
  }

  75% {
    opacity: 1;
    transform: translate3d(0, 10px, 0);
  }

  90% {
    opacity: 1;
    transform: translate3d(0, -5px, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
@keyframes sevenrotateinr{
  from {
    transform-origin: center;
    transform:translateX(-100%) rotate(-360deg);
    opacity: 0;
  }

  to {
    transform-origin: center;
    opacity: 1;
  }
}
@keyframes sevenfadeinl{
  from {
    opacity: 0;
    transform: translate3d(-100%, 0, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
@keyframes sevenbounceinl{
  from,
  60%,
  75%,
  90%,
  to {
    animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
  }

  0% {
    opacity: 0;
    transform: translate3d(-3000px, 0, 0);
  }

  60% {
    opacity: 1;
    transform: translate3d(25px, 0, 0);
  }

  75% {
    opacity: 1;
    transform: translate3d(-10px, 0, 0);
  }

  90% {
    opacity: 1;
    transform: translate3d(5px, 0, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
@keyframes sevendiao{
  from{
    opacity: 0;
    transform: translateY(-100rpx)
  }
  20%{
    opacity: 1;
  }
  to{
    opacity: 1;
  }
}
@keyframes sevenzhuan{
  10%{
    transform: rotate(25deg);
  }
  30%{
    transform: rotate(-25deg);
  }
  50%{
    transform: rotate(12deg);
  }
  70%{
    transform: rotate(-12deg);
  }
  80%{
    transform: rotate(0);
  }
  100%{
    transform: rotate(0);
  }
}
@keyframes sevenflipiny{
from {
    transform: perspective(400px) rotate3d(0, 1, 0, 90deg);
    animation-timing-function: ease-in;
    opacity: 0;
  }

  40% {
    transform: perspective(400px) rotate3d(0, 1, 0, -20deg);
    animation-timing-function: ease-in;
  }

  60% {
    transform: perspective(400px) rotate3d(0, 1, 0, 10deg);
    opacity: 1;
  }

  80% {
    opacity: 1;
    transform: perspective(400px) rotate3d(0, 1, 0, -5deg);
  }

  to {
    opacity: 1;
    transform: perspective(400px);
  }
}
@keyframes sevenbounceinr{
  from,
  60%,
  75%,
  90%,
  to {
    animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1);
  }

  from {
    opacity: 0;
    transform: translate3d(3000px, 0, 0);
  }

  60% {
    opacity: 1;
    transform: translate3d(-25px, 0, 0);
  }

  75% {
    opacity: 1;
    transform: translate3d(10px, 0, 0);
  }

  90% {
    opacity: 1;
    transform: translate3d(-5px, 0, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(0, 0, 0);
  }
}
@keyframes sevenfadedownscale{
  from{
    transform: scale(0.5) translateY(-50%);
  }
  50%{
    opacity: 1;
    transform: scale(0.7) translateY(-10%);
  }
  to{
    opacity: 1;
  }
}